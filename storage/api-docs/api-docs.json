                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "user_type",
                        "in": "query",
                        "description": "User type ['admin', 'medical', 'patient']",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "admin",
                                "medical",
                                "patient"
                            ]
                        }
                    },
                    {
                        "name": "medical_crm",
                        "in": "query",
                        "description": "User type 'medical' or 'admin'",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "date_of_birth_patient",
